#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++){                    //for number of rows
        for(int j=0;j<i;j++){                //for space
            printf(" ");
        }
        for(int k=1;k<=(n-i)+1;k++){         //for stars
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
