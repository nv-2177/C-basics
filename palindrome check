#include <stdio.h>
#include<string.h>
int main() {
    char s[100];
    int ispal=1;
    scanf("%s",s);
    int len=strlen(s);
    for(int i=0;i<=len/ 2;i++){
        if(s[i]!=s[len-1-i]){
            ispal=0;
            break;
        }
    }
if(ispal==1){
    printf("palindrome");
}
else{
    printf("not a palindrome");
}
    return 0;
}
